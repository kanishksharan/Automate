# Listing out the indexes in the list()

def listing():
    l = []
    while (True):
        print("Enter the text for the " + str(len(l)) + "th index: \n")
        name = input()

        if name == "q" or name == "Q":
            break
        l.append(name)
    # Now print out the names of the listing
    print("The names are as follows: \n")
    for names in l:
        print(names, "\n")
    print("End of list")

listing()
